<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" class="chart" width="420" height="150" aria-labelledby="title" role="img">
  <title id="title">A bart chart showing WCAG errors</title>
  <desc>{{axeCountsDescription}}</desc>
  <g class="bar">s
    <rect width="{{criticalCountPercent}}%" height="19" style="fill:#ff3838;stroke-width:5;fill-opacity:.9"></rect>
    <text x="{{criticalCountPercent}}%" y="9.5" dy=".35em"> &nbsp; &nbsp; {{criticalCount}} critical errors</text>
  </g>
  <g class="bar">
    <rect width="{{seriousCountPercent}}%" height="19" y="20" style="fill:#ffb302;stroke-width:5;fill-opacity:0.8"></rect>
    <text x="{{seriousCountPercent}}%" y="28" dy=".35em"> &nbsp; &nbsp; {{seriousCount}} serious errors</text>
  </g>
  <g class="bar">
    <rect width="{{moderateCountPercent}}%" height="19" y="40" style="fill:#fce83a;stroke-width:5;fill-opacity:0.7"></rect>
    <text x="{{moderateCountPercent}}%" y="48" dy=".35em"> &nbsp; &nbsp; {{moderateCount}} moderate errors</text>
  </g>
  <g class="bar">
    <rect width="{{minorCountPercent}}%" height="19" y="60" style="fill:#2dccff;stroke-width:5;fill-opacity:0.6"></rect>
    <text x="{{minorCountPercent}}%" y="68" dy=".35em"> &nbsp; &nbsp; {{minorCount}} minor errors</text>
  </g>s
</svg>
