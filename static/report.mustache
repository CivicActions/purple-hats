    <path d="M15 0H8v2h4.6L6.3 8.3l1.4 1.4L14 3.4V8h2V1c0-.6-.4-1-1-1Z" id="Path"/>
    <path d="M14 16H1c-.6 0-1-.4-1-1V2c0-.6.4-1 1-1h4v2H2v11h11v-3h2v4c0 .6-.4 1-1 1Z" id="Path"/>
  </g>
</svg>

</div>

    <div class="main">
      <div id="loader"></div>
      <div class="container-fluid">
        <div class="row" style="visibility:hidden;" id="report">
          <div class="col-12 pt-4 pb-4">
            <table id="a11y-table" class="display" style="width:100%">
              <thead>
                <tr>
                  <th>IMPACT</th>
                  <th class="dt-nowrap">ISSUE</th>
                  <th class="dt-nowrap">PAGE</th>
                  <th class="element">HTML ELEMENT <span>Hover to view full</span></th>
                  <th class="dt-nowrap text-center">HELP URL</th>
                  <th class="dt-nowrap text-center">FIXED?</th>
                </tr>
              </thead>
              <tbody>
                {{#allissues}}
                <tr>
                  <td><span class="impact {{impact}}"><span class="hidden">{{order}}</span>{{impact}}</span><br>
                    {{#disabilities}}
                      <div class="impact-icon-size">
                        {{{.}}}
                      </div>
                    {{/disabilities}}
                  </td>
                  <td class="issue">{{description}}<br>
                  {{#wcagLinks}}
                  <a class="wcag-clause" href="{{href}}" target="_blank">WCAG {{wcag}}</a>
                  {{/wcagLinks}}
                  </td>
                  <td class="page"><a href="{{url}}" target="_blank">{{page}}</a></td>
                  <td class="element">{{htmlElement}}</td>
                  <td class="text-center dt-nowrap"><a href="{{helpUrl}}" target="_blank">
                    <svg width="16px" height="16px" viewBox="0 0 16 16" version="1.1">
                      <use xlink:href="#help-url-svg"></use>
                    </svg>
                  </a></td>
                  <td class="text-center dt-nowrap">
                  </td>
                </tr>
                {{/allissues}}
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>

    <div class="footer">
      <div class="container-fluid">
        <div class="row text-center pt-2 pb-2">
          <div class="col-sm-6 text-sm-left">
            <hr class="d-sm-none">
          </div>
          <div class="col-sm-6 text-sm-right">
            Purple Hats: initially developed by <a href="https://www.hive.tech.gov.sg/">Government Digital Services Singapore</a>
	          and extended by <a href="https://accessibility.civicactions.com/">CivicActions</a>.
          </div>
        </div>
      </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
    <script src="https://cdn.datatables.net/select/1.3.1/js/dataTables.select.min.js"></script>
